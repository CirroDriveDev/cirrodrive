{
  "name": "@cirrodrive/frontend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "imports": {
    "#app/*": [
      "./src/app/*",
      "./src/app/*.ts"
    ],
    "#components/*": [
      "./src/components/*",
      "./src/components/*.ts"
    ],
    "#hooks/*": [
      "./src/hooks/*",
      "./src/hooks/*.ts"
    ],
    "#pages/*": [
      "./src/pages/*",
      "./src/pages/*.ts"
    ],
    "#services/*": [
      "./src/services/*",
      "./src/services/*.ts"
    ],
    "#shadcn/*": [
      "./src/shadcn/*",
      "./src/shadcn/*.ts"
    ],
    "#store/*": [
      "./src/store/*",
      "./src/store/*.ts"
    ],
    "#types/*": [
      "./src/types/*",
      "./src/types/*.ts"
    ],
    "#utils/*": [
      "./src/utils/*",
      "./src/utils/*.ts"
    ]
  },
  "scripts": {
    "build": "tsc && vite build",
    "build:dev": "tsc && vite build --mode development",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "vite --host",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "preview": "vite preview",
    "test": "vitest",
    "test:ci": "vitest --run --reporter=default --reporter=json",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "ui-add": "shadcn add"
  },
  "prettier": "./.prettierrc.js",
