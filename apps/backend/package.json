    }
  },
  "prettier": "@cirrodrive/prettier-config",
  "private": true,
  "scripts": {
    "build": "tsc -b && vite build --ssr src/server.ts",
    "clean": "git clean -xdf .cache .turbo node_modules",
    "dev": "vite-node --watch src/server.ts",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "start": "vite-node dist/server.js",
    "test": "vitest",
    "test:ci": "vitest run --reporter=default --reporter=json ",
    "test:integration": "vitest --dir test/integrations",
    "test:integration:ci": "vitest run --dir test/integrations --reporter=default --reporter=json",
    "test:unit": "vitest --dir test/units",
    "test:unit:ci": "vitest run --dir test/units --reporter=default --reporter=json",
    "typecheck": "tsc --noEmit"
  },
  "type": "module",
  "version": "0.0.0"
